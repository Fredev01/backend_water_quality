# Test environment configuration
TESTING=true
ENVIRONMENT=test
LOG_LEVEL=DEBUG
SKIP_FIREBASE_INIT=true

FIREBASE_TYPE="service_account"
FIREBASE_API_KEY='xxxxxx'
FIREBASE_REALTIME_URL='https://test-project.firebaseio.com/'
# Firebase test configuration
FIREBASE_DATABASE_URL=https://test-project.firebaseio.com/
FIREBASE_PROJECT_ID=test-project
FIREBASE_PRIVATE_KEY_ID=test-key-id
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nTEST_PRIVATE_KEY_FOR_TESTING_ONLY\n-----END PRIVATE KEY-----\n"
